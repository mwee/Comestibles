<p id="notice"><%= notice %></p>

<p>
  <strong>Owner:</strong>
  <%= @group.owner_id %>
</p>

<p>
  <strong>Name:</strong>
  <%= @group.name %>
</p>

<p>
    <strong>Members:</strong>
    <ul>
        <% for member in @group.members %>
            <li><%= member.name %></li>
        <% end %>
    </ul>
</p>

<%= link_to 'Edit', edit_group_path(@group) %> |
<%= link_to 'Back', groups_path %>
